def whileAandB():
    a = int(input("Enter a number (A) :"))
    b = int(input("Enter another number (B) :"))
    if a < b:
        a, b = b, a
    while a >= b:
        print(b)
        b += 1
whileAandB()

def digits():
    n = int(input("Enter a Number :"))
    if n == 0:
        print("0")
    count = 0
    while n != 0:
        n //= 10
        count += 1
    print(count)
digits()
