def greaterlesser():
    n = int(input("Enter a number :"))
    if n > 75 and n < 100:
        print("Number is greater than 75 but lesser than 100")
    else:
        if n > 50 and n < 75:
            print("Number is greater than 50 but lesser than 75")
        else:
            if n > 25 and n < 50:
                print("Number is greater than 25 but lesser than 50")
            else:
                if n > 0 and n < 25:
                    print("Number is greater than 0 but lesser than 25")
                else:
                    if n < 0:
                        print("Number is negative")
                    else:
                        if n > 100:
                            print("Number is greater than 100")
#greaterlesser()


def examresult():
    g = int(input("Enter percentage :"))
    if g > 100:
        print("PLEASE ENTER A VALID PERCENTAGE")
    elif g >= 75:
        print("TOPPER")
    elif g >= 60:
        print("FIRST DIVISION")
    elif g >= 50 and g <= 59:
        print("SECOND DIVISION")
    elif g >= 40 and g <= 49:
        print("THIRD DIVISION")
    elif g < 40:
        print("FAIL")
#examresult()


def quadrants():
    x = int(input("Enter X Coordinate :"))
    y = int(input("Enter Y Coordinate :"))
    if x>0 and y>0:
        print("Point lies in the First Quadrant")
    elif x<0 and y>0:
        print("Point iles in the Second Quadrant")
    elif x<0 and y<0:
        print("Point lies in the Third Quadrant")
    elif x>0 and y<0:
        print("Point lies in the Fourth Quadrant")
    elif x==0 and y>0:
        print("Point lies on positive Y-axis")
    elif x==0 and y<0 :
        print("Point lies on negative Y-axis")
    elif x>0 and y==0:
        print("Point lies on positive X-axis")
    elif x<0 and y==0:
        print("Point lies on negative X-axis")
    else:
        if x==0 and y==0:
            print("Point lies on the origin (0,0)")
#quadrants()


def zanzibarboat():
    adult_weight = 75
    child_weight = 30
    boat_weight = int(input("Enter max weight the boat can handle :"))
    n_adult = int(input("Enter the number of adults :"))
    n_child = int(input("Enter the number of children :"))
    total_weight = ((adult_weight) * (n_adult)) + ((child_weight) * (n_child))
    if total_weight > boat_weight:
        print("Max Weight :", boat_weight)
        print("Total Weight :", total_weight)
        print("As the Total Weight is more than the Boat Weight, the boat will SINK")
    else:
        print("Max Weight :", boat_weight)
        print("Total Weight :", total_weight)
        print("As the Total Weight is less than the Boat Weight, the boat will NOT SINK")
#zanzibarboat()


def electricitybill():
    units = int(input("Enter the units consumed :"))
    if units <= 200:
        print("Electricity Bill :", 0.5 * units)
    elif units <= 400:
        rem = units % 200
        print("Electricity Bill (with Rs. 100 extra charge): Rs", (200 * 0.5) + (rem * 0.65) + 100)
    elif units <= 600:
        rem = units % 400
        print("Electricity Bill (with Rs. 200 extra charge): Rs", (400 * 0.65) + (rem * 0.8) + 200)
    elif units <= 700:
        rem = units % 700
        print("Electricity Bill (with Rs. 425 extra charge): Rs", (700 * 0.8) + (rem * 1.25) + 425)
    elif units > 700:
        print("You have spent too many units, you are now BANISHED from the Kingdom")
#electricitybill()


