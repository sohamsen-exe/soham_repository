def checkstring():
    s = input("Enter a string :")
    n = input("Enter a letter/number :")
    if n in s:
        print(n, "is in '", s, "'")
    else:
        print(n, "was not found in '", s, "'")
#checkstring()


def smallestoffour():
    n1 = int(input("Enter first number :"))
    n2 = int(input("Enter second number :"))
    n3 = int(input("Enter third number :"))
    n4 = int(input("Enter fourth number :"))
    if n1 < n2 and n1 < n3 and n1 < n4:
        print(n1, "is the smallest number")
    elif n2 < n1 and n2 < n3 and n2 < n4:
        print(n2, "is the smallest number")
    elif n3 < n1 and n3 < n2 and n3 < n4:
        print(n3, "is the smallest number")
    elif n4 < n1 and n4 < n2 and n4 < n3:
        print(n4, "is the smallest number")
#smallestoffour()


def circleinsquare():
    x = int(input("Enter length of side of square :"))
    r = int(input("Enter radius of circle :"))
    if x >= (r * 2):
        print("Side of Square :", x ** 2)
        print("Diameter of Circle :", r * 2)
        print("Circle can fit inside the square")
    else:
        print("Side of Square :", x)
        print("Diameter of Circle :", r * 2)
        print("Circle cannot be inside the square")
#circleinsquare()


def trendynumber():
    global rem
    n = int(input("Enter a number :"))
    s = str(n)
    l_n = len(s)
    if l_n == 3:
        pos = s[1]
        rem = int(pos) % 3
    else:
        print("Invalid Number")
    if l_n == 3 and rem == 0:
        print("\nGiven number is a Trendy Number")
    else:
        print("\nGiven number is not a Trendy Number")
#trendynumber()


def threepsychos():
    x1 = int(input("Enter x-coordinate of First House :"))
    y1 = int(input("Enter y-coordinate of First House :"))
    x2 = int(input("Enter x-coordinate of Second House :"))
    y2 = int(input("Enter y-coordinate of Second House :"))
    print("Coordinates of First house :", x1, ",", y1)
    print("Coordinates of Second House :", x2, ",", y2)
    print("Midpoint of x-coordinates :", ((x1 + x2) / 2))
    print("Midpoint of y-coordinates :", ((y1 + y2) / 2))
#threepsychos()

def swaptwonumbers():
    x = int(input("Enter a number (x) :"))
    y = int(input("Enter another number (y) :"))
    x, y = y, x
    print("X :",x,"\nY :",y)
#swaptwonumbers()

def swaptemp():
    a = int(input("Enter a number (A) :"))
    b = int(input("Enter another number (B) :"))
    temp = a
    a = b
    b = temp
    print("A :", a, "\nB :", b)
#swaptemp()

def swaparithmetic():
    a = int(input("Enter a number (A) :"))
    b = int(input("Enter another number (B) :"))
    a = a+b
    b = a-b
    a = a-b
    print("*"*50, "AFTER REVERSING", "*"*50)
    print("A :", a, "\nB :", b)
#swaparithmetic()

def swaparithmetic_pt2():
    a = int(input("Enter a number (A) :"))
    b = int(input("Enter another number (B) :"))
    a = a * b
    b = a // b
    a = a // b
    print("*" * 50, "AFTER REVERSING", "*" * 50)
    print("A :", a, "\nB :", b)
#swaparithmetic_pt2()

def FtoC():
    f = float(input("Enter temperature in Celsius :"))
    print("Degree in Fahrenheit :", ((f-32) * (5 / 9)))
#FtoC()

def expression():
    a = 5 % 5 + 5 // 5 - 5 * 5
    print(round(a,2))
    b = 6 + 6 // 6 - 6 * 6 ** 6 % 6
    print(round(b,2))
    c = 5 % 10 + 10.5 - 23 * 4.8 // 3
    print(round(c,2))
#expression()

def posorneg():
    n = int(input("Enter a number :"))
    if n > 0:
        print("Given Number is Positive", n * n)
    elif n == 0:
        print("Given Number is Zero")
    else:
        print("Given Number is Negative", n * n * n)
#posorneg()

def libusage():
    n = int(input("Enter the number of days of usage :"))
    if n <= 5:
        print("Fee :", n*2)
    elif n > 5 and n <= 10:
        rem = n-5
        print("Fee :", 10+(rem*3))
    elif n > 10 and n < 15:
        rem = n-10
        print("Fee :", 25+(rem*4))
    elif n >= 15:
        rem = n-15
        print("Fee :", 45+(rem*5))
    elif n < 0:
        print("INVALID NUMBER")
#libusage()

def naturalnumbers():
    n = int(input("Enter the number of natural numbers to print :"))
    for i in range (1, n+1):
        print (i)
#naturalnumbers()

def firstmultiples():
    n = int(input("Enter a number :"))
    m = int(input("Number of Multiples :"))
    for i in range (1,m+1):
        print (n*i)
#firstmultiples()

def innerouterloops():
    for i in range(1,6):
        for j in range(1,i+1):
            print ("*", end = "")
        print()
#innerouterloops()

def alphanum():
    string = input("Enter a string :")
    num_alpha = 0
    num_numeric = 0
    for i in string:
        if i.isalpha():
            num_alpha += 1
        elif i.isdigit():
            num_numeric += 1
        else:
            pass
    print("From the given string:")
    print("Alphabets :", num_alpha)
    print("Numbers :", num_numeric)
#alphanum()

def factorial():
    n = int(input("Enter factorial number :"))
    fact = 1
    for i in range(1, n + 1):
          fact *= i
          print("Step",i,":",fact)
#factorial()

def menudrivenstring():
    def reversestring():
        x = input("Enter a string :")
        print("Reversed String :",x[::-1])
    def tentimes():
        x = input("Enter a String :")
        print(x*10)
    def concatenate():
        x = "Happy"
        z = "I am "
        print( z + x )
    while True:
        print('''
Choose any of the below : 
1. Reverse the String
2. Display a String ten times
3. I am Happy
4. Exit''')
        ch = int(input("Enter your choice :"))
        if ch == 1:
            reversestring()
        elif ch == 2:
            tentimes()
        elif ch == 3:
            concatenate()
        elif ch == 4:
            print("-" * 50, "THANK YOU", "-" * 50)
            break
        else:
            print('*' * 50, "INVALID CHOICE", "*" * 50)
#menudrivenstring()

def median():
    a = int(input("Enter a Number (A):"))
    b = int(input("Enter a Number (B):"))
    c = int(input("Enter a Number (C):"))
    if a < b < c:
        print("Median :", b)
    elif c < b < a:
        print("Median :", b)
    elif a < c < b:
        print("Median :", c)
    elif c < a < b:
        print("Median :", a)
    elif b < a < c:
        print("Median :", a)
    elif b < c < a:
        print("Median :", c)
#median()

def firstoddnums():
    n = int(input("Enter the first n number of odd numbers :"))
    for i in range(0, n+1):
        if i % 2 != 0:
            print ( i )
        else:
            pass
#firstoddnums()

