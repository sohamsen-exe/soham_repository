def population():
    n = 100000
    t = 10
    r = 5
    j = ((5 / 100) * n)
    for i in range(1,11):
        print("Year",i,":")
        n += j
        print(n)
        print()
#population()

def palindrome():
    s = input("Enter a string :")
    l = len(s)
    s1 = ''
    for i in range(-1, -l - 1, -1):
        s1 += s[i]
    print("\nGiven String :",s1,"\n")
    if s1 == s:
        print("Given string is a Palindrome")
    else:
        print("Not a Palindrome")
#palindrome()

def carsandroads():
    a = int(input("Enter value of road A :"))
    b = int(input("Enter value of road B :"))
    c = int(input("Enter value of road C :"))
    x = int(input("Enter number of cars :"))
    if a%x==0 and b%x==0 and c%x==0:
        print("Car can go into all three roads")
    elif a%x==0 and b%x==0:
        print("Car can go into roads A and B")
    elif b%x==0 and c%x==0:
        print("Car can go into roads B and C")
    elif a%x==0 and c%x==0:
        print("Car can go into roads A and C")
    elif a%x==0:
        print("Car goes into road A")
    elif b%x==0:
        print("Car goes into road B")
    elif c%x==0:
        print("Car goes into road C")
    else:
        print("Car cannot go into any of the roads")
#carsandroads()

def mileage():
    m = float(input("Enter mileage of the car (float) :"))
    p = int(input("Enter how many liters of petrol is left :"))
    d = int(input("Enter distance to be travelled :"))
    max_dist = m * p
    if max_dist > d:
        print("\nMax distance the car can travel with current petrol :", max_dist)
        print("Distance left to travel :", d)
        print("\nThe destination will be reached")
    elif max_dist == d:
        print("\nMax distance the car can travel with current petrol :", max_dist)
        print("Distance left to travel :", d)
        print("\nThe destination will be reached")
    else:
        print("\nMax distance the car can travel with current petrol :", max_dist)
        print("Distance left to travel :", d)
        print("\nThe destination cannot be reached")
#mileage()

def swimmingpools():
    lc = int(input("Enter length of the cube :"))
    wv = int(input("Enter water volume :"))
    cv = lc ** 3
    pv = cv * 1000
    if pv >= wv:
        print("\nPool can store the water")
    else:
        print("\nPool cannot store the water")
#swimmingpools()

def FACElabs():
    x = int(input("Enter the seating capacity of Lab 1 :"))
    y = int(input("Enter the seating capacity of Lab 2 :"))
    z = int(input("Enter the seating capacity of Lab 3 :"))
    face = input("Enter the lab allocated for FACE training :")

    if face == "l1" or face == "L1" or face == "1":
        if y<z:
            print("\nLab 2 has minimal seating capacity")
        else:
            print("\nLab 3 has minimal seating capacity")
    elif face == "l2" or face == "L2" or face == '2':
        if x<z:
            print("\nLab 1 has minimal seating capacity")
        else:
            print("\nLab 3 has minimal seating capacity")
    elif face == 'l3' or face == 'L3' or face == '3':
        if x<y:
            print("\nLab 1 has minimal seating capacity")
        else:
            print("\nLab 2 has minimal seating capacity")
    else:
        print()
#FACElabs()

def CSElabs():
    x = int(input("Enter capacity of Lab 1 :"))
    y = int(input("Enter capactity of Lab 2 :"))
    z = int(input("Enter capacity of Lab 3 :"))
    n = int(input("Enter number of students :"))

    if n <= x and n <= y and n <= z:
        print("\nLabs 1, 2 and 3 can accomodate the students")
    elif n <= x and n <= y:
        print("\nLabs 1 and 2 can accomodate the students")
    elif n <= y and n <= z:
        print("\nLabs 2 and 3 can accomodate the students")
    elif n <= x and n <= z:
        print("\nLabs 1 and 3 can accomodate the students")
    elif n <= x:
        print("\nLab 1 can accomodate the students")
    elif n <= y:
        print("\nLab 2 can accomodate the students")
    elif n <= z:
        print("\nLab 3 can accomodate the students")
    else:
        print("\nNone of the labs can accomodate the students")
#CSElabs()

def movietickets():
    age = int(input("Enter age :"))
    print('''
First Show : 10.15
Second Show : 13.30
Third Show (matinee) : 18.00
Fourth Show (matinee) : 22.00''')
    mov_time = float(input("\nEnter timings of the show :"))
    if age <= 13:
        if mov_time == 10.15:
            print("\nTicket Price : $4.00")
        elif mov_time == 13.30:
            print("\nTicket Price : $4.00")
        elif mov_time == 18.00:
            print("\nTicket Price : $2.00")
        elif mov_time == 22.00:
            print("\nTicket Price : $2.00")
        else:
            print("\nNo movie is playing at that time")
    else:
        if mov_time == 10.15:
            print("\nTicket Price : $8.00")
        elif mov_time == 13.30:
            print("\nTicket Price : $8.00")
        elif mov_time == 18.00:
            print("\nTicket Price : $5.00")
        elif mov_time == 22.00:
            print("\nTicket Price : $5.00")
        else:
            print("\nNo movie is playing at that time")
#movietickets()

def salary():
    mon = int(input("Enter hours worked on Monday :"))
    tue = int(input("Enter hours worked on Tuesday :"))
    wed = int(input("Enter hours worked on Wednesday :"))
    thu = int(input("Enter hours worked on Thursday :"))
    fri = int(input("Enter hours worked on Friday :"))
    sat = int(input("Enter hours worked on Saturday :"))
    sun = int(input("Enter hours worked on Sunday :"))
    sal = 0
    week = [mon, tue, wed, thu, fri, sat, sun]
    for i in week:
        if i <= 8:
            sal += i * 100
        elif i > 8:
            sal += 800 + ((i - 8) * 15)


def age():
    birth = int(input("Enter birth year :"))
    current = int(input("Enter current year :"))

    if birth > current:
        print((100 - birth) + current)
    else:
        print(current - birth)
#age()

def workhard():
    s = int(input("Enter salary :"))
    a = int(input("Enter amount paid to the bank :"))
    h = int(input("Enter hostel fees :"))
    f = int(input("Enter food expenses :"))
    p = int(input("Enter amount send to parents :"))

    if (a + h + f + p) > s:
        print("He can save the money")
    elif (a + h + f + p) == s:
        print("He can manage the money")
    elif (a + h + f + p) < s:
        print("He broke")
#workhard()

def discounts():
    flip_amt = int(input("Enter amount on Flipkart :"))
    flip_disc = int(input("Enter discount in Flipkart :"))
    flip_chr = int(input("Enter shipping charges in Flipkart :"))

    snap_amt = int(input("\nEnter amount on Snapdeal :"))
    snap_disc = int(input("Enter discount in Snapdeal :"))
    snap_chr = int(input("Enter shipping charges in Snapdeal :"))

    amaz_amt = int(input("\nEnter amount on Amazon :"))
    amaz_disc = int(input("Enter discount in Amazon :"))
    amaz_chr = int(input("Enter shipping charges in Amazon :"))

    flip_price = flip_amt + flip_chr - ((flip_amt * flip_disc) // 100)
    snap_price = snap_amt + snap_chr - ((snap_amt * snap_disc) // 100)
    amaz_price = amaz_amt + amaz_chr - ((amaz_amt * amaz_disc) // 100)

    print("\nFlipkart :", flip_price)
    print("Snapdeal :", snap_price)
    print("Amazon :", amaz_price)

    if flip_price <= snap_price and flip_price <= amaz_price:
        print("He will prefer Flipkart")
    elif snap_price <= flip_price and snap_price <= amaz_price:
        print("He will prefer Snapdeal")
    elif amaz_price <= flip_price and amaz_price <= snap_price:
        print("He will prefer Amazon")
#discounts()

def doratrees():
    rows = int(input("Enter number of rows :"))
    cols = int(input("Enter number of columns :"))
    tree_no = int(input("Enter tree number :"))

    total_tree = rows * cols
    col = (tree_no - 1) // rows + 1

    if tree_no >= 1 and tree_no <= total_tree:
        if col == 2 or col == col - 1:
            print("It is a Mango Tree")
        else:
            print("It is not a Mango Tree")
#doratrees()

def FeeAmount():
    s_type = input("Enter Student Type (Merit / Management) :")
    if s_type == "Merit":
        f_type = input("Enter Fee Type (MSDS / MSH) :")
        if f_type == "MSDS":
            TF = float(input("Enter Tuition Fees :"))
            BF = float(input("Enter Bus Fees :"))
            print(round((TF + BF), 2))
        elif f_type == "MSH":
            TF = float(input("Enter Tuition Fees :"))
            HF = float(input("Enter Hostel Fees :"))
            print(round((TF + HF), 2))
    elif s_type == "Management":
        f_type = input("Enter Fee Type (MGSDS / MGSH) :")
        if f_type == "MGSDS":
            TF = float(input("Enter Tuition Fees :"))
            BF = float(input("Enter Bus Fees :"))
            TF = 1.5 * TF
            print(round((TF + BF), 2))
        elif f_type == "MGSH":
            TF = float(input("Enter Tuition Fees :"))
            HF = float(input("Enter Hostel Fees :"))
            TF = 1.5 * TF
            print(round((TF + BF), 2))
#FeeAmount()
